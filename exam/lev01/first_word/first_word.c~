#include <unistd.h>

int		main(int ac, char **av)
{
	int i;
 
	i = 0;
	if (ac != 2 || av[1][i + 1] == '\0')
	{
		write(1, "\n", 1);
		return (0);
	}
	else
	{
		if(av[1][i] == " ")
			i++;
		else
		{
			while(av[1][i] != " ")
			{
				write(1, &ac[1][i], 1);
				i++;
			}
		}
	}
	write(1, "\n", 1);
	return (0);
}
