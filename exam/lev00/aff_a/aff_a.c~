#include <unistd.h>

int		main(int argc, char **argv)
{
	char a;
	int i;

	a = 'a';
	i = 0;
	if (argc != 1)
		return (0);
	while(argv[1][i] != a)
		a++;
	if (argv[1][i] == a)
		write(1, &a, 1);
	return (0);
}
